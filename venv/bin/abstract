#!/Users/lucascoppio/Projects/virtual-machine/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'AbstractLang','console_scripts','abstract'
__requires__ = 'AbstractLang'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('AbstractLang', 'console_scripts', 'abstract')()
    )
